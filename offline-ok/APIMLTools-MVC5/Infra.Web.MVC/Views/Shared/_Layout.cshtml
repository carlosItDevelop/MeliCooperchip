@using System.Web.UI.HtmlControls
@using SDK
<!DOCTYPE html>
<html>
<head>


    <!-- Basic -->
    <meta charset="utf-8" />
    <title>APIML | Tools - Management</title>
    <meta name="keywords" content="HTML5 Template,  100% utilizada neste gerenciador de vendas do Mercadolivre" />
    <meta name="description" content="Sistema gerenciador de vendas no Mercadolivre, utilizando suas API (s)" />
    <meta name="author" content="shopcooperchip.com.br Carlos Alberto dos Santos PoetaRJ" />

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css" />



    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>


    <!---  BODY CODIGO PERSONALIZADO ANTES DO REDERBODY  /-->



    <div class="body">
        <!-- INICIO DO HEADER
        ================================================================================================/ -->

        <div class="navbar-fixed-top">

            <header id="header">



                <div class="container">
                    <div class="logo">
                        <a href="#">
                            <img alt="APIML | Tools" width="111" height="54" data-sticky-width="82" data-sticky-height="40" src="~/tpl/img/logo.png">
                        </a>
                    </div>
                    <div class="search">
                        <form id="searchForm" action="#" method="get">
                            <div class="input-group">
                                <input type="text" class="form-control search" name="q" id="q" placeholder="Buscar..." />
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="submit"><i class="fa fa-search"></i></button>
                                </span>
                            </div>
                        </form>
                    </div>
                    <ul class="social-icons">
                        <li class="facebook"><a href="https://www.facebook.com/ShopCooperchip/" target="_blank" title="Facebook">Facebook</a></li>
                        <li class="twitter"><a href="http://www.twitter.com/ptcooperchip/" target="_blank" title="Twitter">Twitter</a></li>
                        <li class="linkedin"><a href="https://www.linkedin.com/profile/view?id=221584014/" target="_blank" title="Linkedin">Linkedin</a></li>
                    </ul>
                    <nav>
                        <ul class="nav nav-pills nav-top">

                            <li class="signin" id="headerLogin">
                                <i class="fa fa-user"></i> @Html.Partial("_LoginPartial")
